{
  "name": "@endpass/ui",
  "version": "0.14.1",
  "description": "Ui components",
  "author": "Endpass, Inc",
  "homepage": "http://endpass.com/",
  "license": "MIT",
  "main": "endpass-ui.umd.js",
  "module": "endpass-ui.esm.js",
  "repository": "https://github.com/endpass/endpass-core/tree/master/packages/ui",
  "scripts": {
    "dev": "yarn storybook",
    "update": "yarn build && npm publish ./dist",
    "build": "yarn clear && yarn build:dist && node ./copy",
    "build:dist": "rollup -c --environment BUILD:production",
    "build:watch": "yarn build:dist -w",
    "clear": "rimraf ./dist",
    "test": "jest",
    "coverage": "yarn test --coverage",
    "pre-commit": "lint-staged",
    "lint": "eslint '**/*.{js,vue,json}' --quiet",
    "format:prettier:vue": "prettier --write '**/*.vue'",
    "format:eslint": "eslint --ext .json,.js,.vue src/ --fix --quiet || exit 0",
    "format:prettier": "prettier --write '**/*.{js,json}'",
    "format": "yarn format:prettier:vue && yarn format:eslint && yarn format:prettier",
    "storybook": "start-storybook -s public",
    "storybook:build": "build-storybook"
  },
  "bugs": {
    "url": "https://github.com/endpass/endpass-core/issues"
  },
  "copyToDist": [
    "src",
    "public",
    "README.md",
    "package.json",
    "yarn.lock"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json}": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ],
    "*.vue": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ],
    "*.scss": [
      "stylelint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "@endpass/utils": "^1.3.5",
    "vue": "^2.5.21"
  },
  "devDependencies": {
    "@babel/core": "^7.4.0",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/preset-env": "^7.4.1",
    "@babel/runtime": "^7.4.0",
    "@storybook/addon-actions": "^5.0.11",
    "@storybook/addon-storyshots": "^5.0.11",
    "@storybook/vue": "^5.1.9",
    "@vue/test-utils": "^1.0.0-beta.28",
    "autoprefixer": "^9.6.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-dynamic-import-node": "^2.2.0",
    "babel-plugin-require-context-hook": "^1.0.0",
    "babel-preset-vue": "^2.0.2",
    "eslint": "^5.15.3",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-json": "^1.4.0",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-vue": "^5.2.2",
    "find": "^0.3.0",
    "fs-extra": "^7.0.1",
    "husky": "^1.3.1",
    "jest": "^23.6.0",
    "jest-serializer-vue": "^2.0.2",
    "jest-transform-stub": "^1.0.0",
    "jest-vue-preprocessor": "^1.5.0",
    "lint-staged": "^8.1.5",
    "node-sass": "^4.11.0",
    "postcss": "^7.0.17",
    "pre-commit": "^1.2.2",
    "prettier": "^1.16.4",
    "raw-loader": "^3.0.0",
    "rimraf": "^2.6.3",
    "rollup": "^1.7.0",
    "rollup-plugin-alias": "^1.5.1",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^9.2.1",
    "rollup-plugin-sass": "^1.2.2",
    "rollup-plugin-string": "^2.0.2",
    "rollup-plugin-vue": "^4.6.1",
    "sass-loader": "^7.1.0",
    "stylelint": "^10.0.1",
    "stylelint-config-recommended-scss": "^3.3.0",
    "stylelint-scss": "^3.6.1",
    "vue-jest": "^3.0.2",
    "vue-loader": "^15.7.0",
    "vue-template-compiler": "^2.5.21"
  },
  "publishConfig": {
    "access": "public"
  }
}
