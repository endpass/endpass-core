{
  "name": "@endpass/ui",
  "version": "0.5.0",
  "description": "Ui components",
  "author": "Endpass, Inc",
  "homepage": "http://endpass.com/",
  "license": "MIT",
  "main": "endpass-ui.umd.js",
  "module": "endpass-ui.esm.js",
  "repository": "https://github.com/endpass/endpass-core/tree/master/packages/ui",
  "scripts": {
    "update": "yarn build && npm publish ./dist",
    "build": "yarn clear && yarn build:dist && node ./copy",
    "build:dist": "rollup -c --environment BUILD:production",
    "build:watch": "yarn build:dist -w",
    "clear": "rimraf ./dist",
    "test": "jest",
    "pre-commit": "lint-staged",
    "lint": "eslint '**/*.{js,vue,json}' --quiet",
    "format:prettier:vue": "prettier --write '**/*.vue'",
    "format:eslint": "eslint --ext .json,.js,.vue src/ --fix --quiet",
    "format:prettier": "prettier --write '**/*.{js,json}'",
    "format": "yarn format:prettier:vue && yarn format:eslint && yarn format:prettier"
  },
  "bugs": {
    "url": "https://github.com/endpass/endpass-core/issues"
  },
  "copyToDist": [
    "src",
    "README.md",
    "package.json",
    "yarn.lock"
  ],
  "pre-commit": [
    "pre-commit"
  ],
  "lint-staged": {
    "*.{js,json,vue}": [
      "yarn format",
      "git add"
    ]
  },
  "dependencies": {
    "@endpass/utils": "^1.3.5",
    "vue": "^2.5.21"
  },
  "devDependencies": {
    "@babel/core": "^7.4.0",
    "@babel/preset-env": "^7.4.1",
    "@babel/runtime": "^7.4.0",
    "@vue/test-utils": "^1.0.0-beta.28",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-plugin-dynamic-import-node": "^2.2.0",
    "eslint": "^5.15.3",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-json": "^1.4.0",
    "eslint-plugin-vue": "^5.2.2",
    "find": "^0.3.0",
    "fs-extra": "^7.0.1",
    "jest": "^23.6.0",
    "jest-serializer-vue": "^2.0.2",
    "jest-transform-stub": "^1.0.0",
    "lint-staged": "^8.1.5",
    "node-sass": "^4.11.0",
    "pre-commit": "^1.2.2",
    "prettier": "^1.16.4",
    "rimraf": "^2.6.3",
    "rollup": "^1.7.0",
    "rollup-plugin-alias": "^1.5.1",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^9.2.1",
    "rollup-plugin-string": "^2.0.2",
    "rollup-plugin-vue": "^4.6.1",
    "vue-jest": "^3.0.2",
    "vue-template-compiler": "^2.5.21"
  },
  "publishConfig": {
    "access": "public"
  }
}
