{
  "name": "@endpass/ui",
  "version": "0.15.25",
  "description": "UI components",
  "author": "Endpass, Inc",
  "homepage": "http://endpass.com/",
  "license": "MIT",
  "main": "endpass-ui.umd.js",
  "module": "endpass-ui.esm.js",
  "repository": "https://github.com/endpass/endpass-core/tree/master/packages/ui",
  "scripts": {
    "dev": "yarn dev:svg & yarn storybook",
    "dev:svg": "gulp svg:dev",
    "update": "yarn build && npm publish ./dist",
    "build": "yarn clear && yarn build:svg && yarn build:dist && node ./copy",
    "build:dist": "rollup -c --environment BUILD:production",
    "build:watch": "yarn build:dist -w",
    "build:svg": "gulp svg",
    "clear": "rimraf ./dist",
    "test": "jest",
    "coverage": "yarn test --coverage",
    "pre-commit": "lint-staged",
    "lint": "eslint '**/*.{js,vue,json}' --quiet",
    "format:prettier:vue": "prettier --write '**/*.vue'",
    "format:eslint": "eslint --ext .json,.js,.vue src/ --fix --quiet || exit 0",
    "format:prettier": "prettier --write '**/*.{js,json}'",
    "format": "yarn format:prettier:vue && yarn format:eslint && yarn format:prettier",
    "storybook": "start-storybook -s public",
    "storybook:build": "build-storybook",
    "gen:component": "hygen component new --name",
    "gen:atom": "hygen atom new --name"
  },
  "bugs": {
    "url": "https://github.com/endpass/endpass-core/issues"
  },
  "copyToDist": [
    "src",
    "public",
    "README.md",
    "package.json",
    "yarn.lock"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json}": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ],
    "*.vue": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ],
    "*.scss": [
      "stylelint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "@endpass/utils": "^1.8.1",
    "vue": "^2.5.21"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/runtime": "^7.5.5",
    "@storybook/addon-actions": "^5.1.9",
    "@storybook/addon-storyshots": "^5.1.9",
    "@storybook/vue": "^5.1.9",
    "@vue/test-utils": "^1.0.0-beta.28",
    "autoprefixer": "^9.6.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.2",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-dynamic-import-node": "^2.3.0",
    "babel-plugin-require-context-hook": "^1.0.0",
    "babel-preset-vue": "^2.0.2",
    "css-loader": "^3.1.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-json": "^1.4.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-vue": "^5.2.3",
    "find": "^0.3.0",
    "fs-extra": "^8.1.0",
    "gulp": "^4.0.2",
    "gulp-plumber-error-handler": "^1.0.0",
    "gulp-rename": "^1.4.0",
    "gulp-svgmin": "^2.2.0",
    "gulp-svgstore": "^7.0.1",
    "husky": "^3.0.1",
    "hygen": "^4.0.7",
    "jest": "^23.6.0",
    "jest-serializer-vue": "^2.0.2",
    "jest-transform-stub": "^2.0.0",
    "jest-vue-preprocessor": "^1.5.0",
    "lint-staged": "^8.2.1",
    "node-sass": "^4.12.0",
    "postcss": "^7.0.17",
    "pre-commit": "^1.2.2",
    "prettier": "^1.18.2",
    "raw-loader": "^3.1.0",
    "rimraf": "^2.6.3",
    "rollup": "^1.17.0",
    "rollup-plugin-alias": "^1.5.2",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.0.1",
    "rollup-plugin-sass": "^1.2.2",
    "rollup-plugin-string": "^2.0.2",
    "rollup-plugin-vue": "^5.0.1",
    "sass-loader": "^7.1.0",
    "stylelint": "^10.1.0",
    "stylelint-config-recommended-scss": "^3.3.0",
    "stylelint-scss": "^3.9.2",
    "vue-jest": "^3.0.4",
    "vue-loader": "^15.7.1",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^4.37.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
