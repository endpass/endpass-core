{
  "name": "@endpass/e2e-utils",
  "version": "0.5.1",
  "description": "Utilities for e2e testing",
  "author": "Endpass, Inc",
  "homepage": "http://endpass.com/",
  "license": "MIT",
  "main": "index.js",
  "module": "index.js",
  "publishConfig": {
    "access": "public"
  },
  "bugs": {
    "url": "https://github.com/endpass/endpass-core/issues"
  },
  "scripts": {
    "update": "yarn build && npm publish ./dist",
    "build": "yarn clear && yarn build:dist && yarn build:worker",
    "build:dev": "rollup -c",
    "build:dist": "NODE_ENV=production rollup -c ./rollup.config.js",
    "build:worker": "node ./scripts/updateWorkerVersion.js",
    "clear": "rimraf ./dist",
    "test": "npm run test:unit",
    "test:unit": "jest ./tests",
    "format:prettier": "prettier --write '**/*.{js,json}'",
    "format:eslint": "vue-cli-service lint --ext .json,.js, src/ --fix --quiet || exit 0",
    "format": "yarn format:eslint && yarn format:prettier",
    "lint": "eslint --fix './src/**/*.js'"
  },
  "lint-staged": {
    "*.{js,json}": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@babel/runtime": "^7.5.5",
    "@endpass/class": "^0.17.1",
    "nanoid": "^2.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@endpass/eslint-plugin-endpass": "^0.1.2",
    "ejs": "^2.6.2",
    "eslint": "^6.1.0",
    "fast-glob": "^3.0.4",
    "husky": "^3.0.2",
    "jest": "^24.8.0",
    "lint-staged": "^9.2.1",
    "prettier": "^1.18.2",
    "rimraf": "^2.6.3",
    "rollup": "^1.19.4",
    "rollup-plugin-alias": "^1.5.2",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.0.2",
    "rollup-plugin-copy": "^3.1.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.2.0"
  }
}
