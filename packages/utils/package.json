{
  "name": "@endpass/utils",
  "version": "1.10.1",
  "description": "Utils and helper functions",
  "author": "Endpass, Inc",
  "homepage": "http://endpass.com/",
  "license": "MIT",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/endpass/endpass-core.git"
  },
  "scripts": {
    "update": "yarn build && npm publish ./dist",
    "build": "yarn clear && yarn build:dist",
    "build:dist": "rollup -c ./rollup.config.js",
    "clear": "rimraf ./dist",
    "test": "npm run unit",
    "unit": "jest --config ./jest.conf.js",
    "format:eslint": "eslint --ext .json,.js src/ --fix",
    "format:prettier": "prettier --write '**/*.{js,json}'",
    "format": "yarn format:eslint && yarn format:prettier",
    "lint": "eslint --fix \"./src/**/*.js\""
  },
  "bugs": {
    "url": "https://github.com/endpass/endpass-core/issues"
  },
  "lint-staged": {
    "*.{js,json}": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "bip39": "^3.0.2",
    "bs58check": "^2.1.2",
    "dayjs": "^1.8.16",
    "eth-ecies": "^1.0.3",
    "ethereumjs-wallet": "^0.6.3",
    "export-files": "^2.1.1",
    "fingerprintjs2": "1.8.6",
    "keythereum": "^1.0.4",
    "lodash": "^4.17.15",
    "node-notifier": "^5.4.3",
    "secp256k1": "^3.7.1"
  },
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.6.3",
    "@babel/runtime": "^7.6.3",
    "@endpass/eslint-plugin-endpass": "^0.2.9",
    "@types/fingerprintjs2": "^2.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.3",
    "eslint": "^6.6.0",
    "fast-glob": "^3.1.0",
    "husky": "^3.0.9",
    "jest": "^24.9.0",
    "lint-staged": "^9.4.2",
    "prettier": "^1.18.2",
    "rimraf": "^3.0.0",
    "rollup": "^1.21.2",
    "rollup-plugin-alias": "^2.0.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-copy": "^3.1.0",
    "typescript": "^3.6.4"
  },
  "publishConfig": {
    "access": "public"
  }
}
