{
  "name": "@endpass/utils",
  "version": "1.3.1",
  "description": "Utils and helper functions",
  "author": "Endpass, Inc",
  "homepage": "http://endpass.com/",
  "license": "MIT",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/endpass/endpass-core.git"
  },
  "scripts": {
    "update": "yarn build && npm publish ./dist",
    "build": "yarn clear && yarn build:dist",
    "build:dist": "rollup -c ./rollup.config.js",
    "clear": "rimraf ./dist",
    "test": "npm run unit",
    "unit": "jest --config ./jest.conf.js",
    "format": "prettier --write \"src/**/*.{js,json}\"",
    "lint": "eslint --fix \"./src/**/*.js\""
  },
  "bugs": {
    "url": "https://github.com/endpass/endpass-core/issues"
  },
  "lint-staged": {
    "./src/**/*.js": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "bs58check": "^2.1.2",
    "dayjs": "^1.7.8",
    "ethereumjs-wallet": "^0.6.2",
    "export-files": "^2.1.1",
    "keythereum": "^1.0.4",
    "lodash": "^4.17.11",
    "node-notifier": "^5.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.0",
    "@babel/plugin-transform-runtime": "^7.4.0",
    "@babel/preset-env": "^7.4.2",
    "@babel/runtime": "^7.4.2",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.15.3",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-prettier": "^3.0.1",
    "find": "^0.3.0",
    "husky": "^1.3.1",
    "jest": "^24.5.0",
    "lint-staged": "^8.1.5",
    "prettier": "^1.16.4",
    "rimraf": "^2.6.3",
    "rollup": "^1.7.3",
    "rollup-plugin-alias": "^1.5.1",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-copy": "^0.2.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
